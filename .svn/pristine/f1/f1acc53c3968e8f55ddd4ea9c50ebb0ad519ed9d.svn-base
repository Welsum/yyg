package yyg.zdxh.entitys;

import java.util.Date;
import java.sql.Timestamp;

import yyg.zdxh.util.UUIDGenerator;

public class BuyCode implements java.io.Serializable{
	private String bcid;
	private String bcsgid;
	private String bcbatch;
	private String bcprid;
	private String bccode;
	private Date addTime;
	public BuyCode() {}
	
	public String getBcid() {
		return bcid;
	}

	public void setBcid(String bcid) {
		this.bcid = bcid;
	}

	public String getBcsgid() {
		return bcsgid;
	}

	public void setBcsgid(String bcsgid) {
		this.bcsgid = bcsgid;
	}

	public String getBcbatch() {
		return bcbatch;
	}

	public void setBcbatch(String bcbatch) {
		this.bcbatch = bcbatch;
	}

	public String getBcprid() {
		return bcprid;
	}

	public void setBcprid(String bcprid) {
		this.bcprid = bcprid;
	}

	public String getBccode() {
		return bccode;
	}

	public void setBccode(String bccode) {
		this.bccode = bccode;
	}

	public Date getAddTime() {
		return addTime;
	}

	public void setAddTime(Date addTime) {
		this.addTime = addTime;
	}

	public BuyCode(String bcid, String bcsgid, String bcbatch, String bcprid, String bccode, Date addTime) {
		super();
		this.bcid = bcid;
		this.bcsgid = bcsgid;
		this.bcbatch = bcbatch;
		this.bcprid = bcprid;
		this.bccode = bccode;
		this.addTime = addTime;
	}

	@Override
	public String toString() {
		return "BuyCode [bcid=" + bcid + ", bcsgid=" + bcsgid + ", bcbatch=" + bcbatch + ", bcprid=" + bcprid
				+ ", bccode=" + bccode + ", addTime=" + addTime + "]";
	}

	

	
	


}
