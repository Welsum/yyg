package yyg.zdxh.entitys;

import java.util.Date;

public class Receive implements java.io.Serializable {
	
	private String rid;
	private String ruid;
	private String rlocationcode;
	private String rrowaddress;
	private String rpostcode;
	private String raddress;
	private String rphone;
	private String rremark;
	private Date adddate;
	public String getRid() {
		return rid;
	}
	public void setRid(String rid) {
		this.rid = rid;
	}
	public String getRuid() {
		return ruid;
	}
	public void setRuid(String ruid) {
		this.ruid = ruid;
	}
	public String getRlocationcode() {
		return rlocationcode;
	}
	public void setRlocationcode(String rlocationcode) {
		this.rlocationcode = rlocationcode;
	}
	public String getRrowaddress() {
		return rrowaddress;
	}
	public void setRrowaddress(String rrowaddress) {
		this.rrowaddress = rrowaddress;
	}
	public String getRpostcode() {
		return rpostcode;
	}
	public void setRpostcode(String rpostcode) {
		this.rpostcode = rpostcode;
	}
	public String getRaddress() {
		return raddress;
	}
	public void setRaddress(String raddress) {
		this.raddress = raddress;
	}
	public String getRphone() {
		return rphone;
	}
	public void setRphone(String rphone) {
		this.rphone = rphone;
	}
	public String getRremark() {
		return rremark;
	}
	public void setRremark(String rremark) {
		this.rremark = rremark;
	}
	public Date getAdddate() {
		return adddate;
	}
	public void setAdddate(Date adddate) {
		this.adddate = adddate;
	}
	@Override
	public String toString() {
		return "Assessment [rid=" + rid + ", ruid=" + ruid + ", rlocationcode=" + rlocationcode + ", rrowaddress="
				+ rrowaddress + ", rpostcode=" + rpostcode + ", raddress=" + raddress + ", rphone=" + rphone
				+ ", rremark=" + rremark + ", adddate=" + adddate + "]";
	}
	public Receive(String rid, String ruid, String rlocationcode, String rrowaddress, String rpostcode,
			String raddress, String rphone, String rremark, Date adddate) {
		super();
		this.rid = rid;
		this.ruid = ruid;
		this.rlocationcode = rlocationcode;
		this.rrowaddress = rrowaddress;
		this.rpostcode = rpostcode;
		this.raddress = raddress;
		this.rphone = rphone;
		this.rremark = rremark;
		this.adddate = adddate;
	}
	public Receive() {
		super();
	}


}
