package yyg.zdxh.entitys;

import java.util.Date;

public class Picture {
	private String pid;
	private String code;
	private String name;
	private String location;
	private String index;
	private String remark;
	private Date addTime;
	public Picture(){}
	
	public String getPid() {
		return pid;
	}
	
	public void setPid(String pid) {
		this.pid = pid;
	}
	
	public String getCode() {
		return code;
	}
	public void setCode(String code) {
		this.code = code;
	}
	
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getLocation() {
		return location;
	}
	public void setLocation(String location) {
		this.location = location;
	}
	public String getIndex() {
		return index;
	}
	public void setIndex(String index) {
		this.index = index;
	}
	public String getRemark() {
		return remark;
	}
	public void setRemark(String remark) {
		this.remark = remark;
	}
	public Date getAddTime() {
		return addTime;
	}
	public void setAddTime(Date addTime) {
		this.addTime = addTime;
	}

	public Picture(String pid, String code, String name, String location, String index, String remark, Date addTime) {
		super();
		this.pid = pid;
		this.code = code;
		this.name = name;
		this.location = location;
		this.index = index;
		this.remark = remark;
		this.addTime = addTime;
	}

	@Override
	public String toString() {
		return "Picture [pid=" + pid + ", code=" + code + ", name=" + name + ", location=" + location + ", index="
				+ index + ", remark=" + remark + ", addTime=" + addTime + "]";
	}

	
	
	
}
